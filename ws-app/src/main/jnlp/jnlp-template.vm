<?xml version="1.0" encoding="$encoding"?>
<jnlp
    spec="$jnlpspec"
    codebase="$jnlpCodebase"
    href="$outputFile">
  <information>
#if($project.Name)
    <title>$project.Name</title>
#end
#if($project.Organization.Name)
    <vendor>$project.Organization.Name</vendor>
#end
#if($project.Url)
    <homepage href="$project.Url"/>
#end
#if($project.Description)
    <description>$project.Description</description>
#end
#if($offlineAllowed)
    <offline-allowed/>
#end
    <shortcut online="true">
      <desktop/>
    </shortcut>
  </information>
#if($allPermissions)
  <security>
     <all-permissions/>
  </security>
#end
  <update check="background" policy="prompt-update"/>
  <resources>
    <j2se version="$j2seVersion" max-heap-size="512m"/>
     $dependencies
     $extensions
    <property name="jnlp.mm.forceInstallPlugins" value="false"/>
    <property name="jnlp.mm.developer" value="false"/>
  </resources>
  <application-desc main-class="$mainClass"/>
</jnlp>
